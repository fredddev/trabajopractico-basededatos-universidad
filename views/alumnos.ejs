<!DOCTYPE html>
<html>
<head>
  <title>Lista de alumnos por instrumento</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body class="bg-light">
    <%- include('partials/menu') %>
  <div class="container mt-5">
    <h1 class="mb-4">Lista de alumnos por instrumento</h1>

    <table class="table table-striped table-bordered">
      <thead class="table-light">
        <tr>
          <th>Instrumento</th>
          <th>CantidadAlumnos</th>          
        </tr>
      </thead>
      <tbody>
        <% alumnosPorInstrumento.forEach(alumno => { %>
          <tr>
            <td><%= alumno.instrumento %></td>
            <td><%= alumno.cantidad_alumnos %></td>            
          </tr>
        <% }) %>
      </tbody>
    </table>
  </div>

</body>
</html>
